{
  "bindings": [
    {
      "name": "myQueueItem",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "%QUEUE%",
      "connection": "STORAGE_CONNECTION_STRING"
    },
    {
      "type": "blob",
      "name": "inputBlob",
      "path": "images/{queueTrigger}",
      "connection": "STORAGE_CONNECTION_STRING",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "thumbnails/thumbnail-{queueTrigger}",
      "connection": "STORAGE_CONNECTION_STRING",
      "direction": "out"
    }
  ],
  "disabled": false
}